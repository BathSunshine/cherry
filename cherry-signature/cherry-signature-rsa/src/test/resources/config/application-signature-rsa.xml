<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-lazy-init="false">

	<bean id="rsaSignature" class=" com.github.cherry.signature.rsa.RsaSignature">
		<constructor-arg name="keyStorePath" value="test.jks"></constructor-arg>
		<constructor-arg name="password" value="password"></constructor-arg>
		<constructor-arg name="gatewayAlias" value=""></constructor-arg>
	</bean>

</beans>